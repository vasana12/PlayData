			System.out.println(); 
		System.out.println("�ѼҼ��� ���� = " + cnt);
	}
	
	static int[] inputPrimeNumber() {
		Scanner in = new Scanner(System.in);
		int [] num = new int[2];
		System.out.print("첫 번째 수 입력 => ");
