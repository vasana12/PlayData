		System.out.print("두 번째 수 입력 => ");
		int num2 = in.nextInt();
		System.out.println();
		
		if (num1 < num2) // �Է� ���� �μ��� ū���� �������� ����
		{
			min = num1;
			max = num2;
		}
		else
		{
			min = num2;
			max = num1;
		}
	}
	static void PrimeNumber() {
