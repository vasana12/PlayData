<!DOCTYPE html>
<html>
	<head>
		<meta charset="EUC-KR">
		<title>Insert title here</title>
			<script>
					var fir = parseInt(prompt('시작 단'));
					var sec = parseInt(prompt('마지막 단'));
					var answer;
					
					if(fir > sec){
						var c = fir;
						fir = sec;
						sec = c;
					}
								
					window.onload = function()
					{
						document.write('<center><table border = "1" align = "center"></center>');	
						document.write('<tr>');
						for(var k = fir; k<= sec; k++){
							document.write('<td>' + k +'단' +'</td>');				
						}
						document.write('</tr>');
						for(var j = 1; j <10; j++){
							document.write('<tr>');		
							for(var i = fir; i <= sec; i++){
								document.write( '<td>' + i + ' x ' + j + ' = ' + i * j + '    </td>');
							}			
							document.write('</tr>');
						}
						document.write('</table>');
					}
			</script>
	</head>
	<body>
		
	</body>
</html>