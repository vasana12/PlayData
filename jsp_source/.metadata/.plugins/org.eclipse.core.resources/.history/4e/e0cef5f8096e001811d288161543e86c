			var fir = prompt('시작 단');
			var sec = prompt('마지막 단');
		
			var answer;
			window.onload = function(){
				
				
			for(var j = 1; j <10; j++){
				document.write('<tr>');		

				for(var i = fir; i < =sec; i++){
					document.write( '<td>' + i + ' x ' + j + ' = ' + i * j + '</td>');
				}			
				document.write('</tr>');
			}
		</script>
