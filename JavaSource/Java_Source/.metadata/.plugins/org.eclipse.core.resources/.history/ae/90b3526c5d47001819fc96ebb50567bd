import java.util.Scanner;

public class PrimeNumber {

	public static void main(String[] args) {
		
		int num1, num2, min, max, cnt=0;
		int i, j;
		
		
		
		for (i=min; i<=max; i++) // ���������� ū�����̿� �����ϴ� �Ҽ��� ����
		{
			for (j=2; j<i; j++) // 2���� i���� ���������� i������ ���� �Ҽ����� �Ǻ��ϱ� ���� �ݺ�
			{
				if (i%j == 0) // i������ ���� ���̻� �Ҽ��� �ƴ�
					break;  
