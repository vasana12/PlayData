package stringOperatroEx;
import static java.lang.System.out;
import static java.lang.System.in;
import java.util.*;

public class Main {

	public static void main(String[] args) {
		StringTokenizer op;
		System.out.println((int)'+');		
		System.out.println((int)'-');	
		System.out.println((int)'*');	
		System.out.println((int)'/');	
		
		Scanner sc = new Scanner(in);
		
		System.out.println("수식 입력 =>");
		op = sc.nextLine();
	}

}
